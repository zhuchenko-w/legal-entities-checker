<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - АРМ проверки юридических лиц</title>
    @Styles.Render("~/Content/css/error")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="outer">
        <div class="middle">
            <div class="col-lg-4 col-md-4 col-sm-3"></div>
            <div class="col-lg-4 col-md-4 col-sm-6 inner">
                @RenderBody()
            </div>
            <div class="col-lg-4 col-md-4 col-sm-3"></div>
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>
